  "tokenColors": [{
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#b2b2b2"
      }
    },
    {
      "name": "keywords",
      "scope": ["keyword", "storage.type.class"],
      "settings": {
        "foreground": "#c5a5c5"
      }
    },
    {
      "name": "functions",
      "scope": "function",
      "settings": {
        "foreground": "#79b6f2"
      }
    },
    {
      "name": "variables",
      "scope": ["variable", "meta.jsx.children"],
      "settings": {
        "foreground": "#d7deea"
      }
    },
    {
      "name": "types",
      "scope": [
        "type",
        "support.type",
        "support.type.property-name.json.comments"
      ],
      "settings": {
        "foreground": "#fac863"
      }
    },
    {
      "name": "Name.Constant",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#8be9ee"
      }
    },
    {
      "name": "Generic: Heading (multi line)",
      "scope": "markup.heading.1 punctuation.definition.heading",
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Name.Entity",
      "scope": "constant.character.entity",
      "settings": {
        "foreground": "#d67c9b"
      }
    },
    {
      "name": "Name.Exception",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#d67c9b"
      }
    },
    {
      "name": "Name.Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.class.builtin",
        "keyword.other.name-of-parameter"
      ],
      "settings": {
        "foreground": "#79b6f2"
      }
    },
    {
      "name": "Name.Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#fc929e"
      }
    },
    {
      "name": "Support.Class",
      "scope": [
        "support.class",
        "support.class.component",
        "constant.numeric",
        "support.variable.property.dom"
      ],
      "settings": {
        "foreground": "#fc929e"
      }
    },
    {
      "name": "Component",
      "scope": ["entity.other.inherited-class", "entity.name.type"],
      "settings": {
        "foreground": "#fac863"
      }
    },
    {
      "name": "Storage.Modifier",
      "scope": ["storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#c5a5c5"
      }
    },
    {
      "name": "Storage.Type",
      "scope": ["constructor"],
      "settings": {
        "foreground": "#79b6f2"
      }
    },
    {
      "name": "Variable.Language",
      "scope": [
        "variable.language",
        "entity.other.attribute-name",
        "constant.language.undefined",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#c5a5c5",
        "fontStyle": ""
      }
    },
    {
      "name": "Constant.Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#ff8b50"
      }
    },
    {
      "name": "Variable.Parameter",
      "scope": [
        "variable.parameter",
        "variable.other.constant",
        "variable.other.readwrite",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#d7deea"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
        "meta.brace.round",
        "meta.array.literal"
      ],
      "settings": {
        "foreground": "#88c6be"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#d7deea"
      }
    },
    {
      "name": "Python Variable",
      "scope": "source.python",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Python Constant.Boolean",
      "scope": "constant.language.python",
      "settings": {
        "foreground": "#f99157"
      }
    },
    {
      "name": "Lua Variable",
      "scope": "source.lua",
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
